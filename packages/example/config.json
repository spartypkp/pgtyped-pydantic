{
    "transforms": [
      {
        "mode": "python",
        "include": "**/*.py",
        "emitTemplate": "{{dir}}/{{name}}_ts.py"
      }    
    ],
    "typesOverrides": {
      "date": {
        "return": "string"
      },
      "int8": "BigInt.t"    
    },
    "srcDir": "./src/",
    "dbUrl": "postgres://postgres:password@localhost/postgres"
  }