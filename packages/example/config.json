{
    "transforms": [
      {
        "mode": "sql",
        "include": "**/*.sql",
        "emitTemplate": "{{dir}}/{{name}}.py"
      }    
    ],
    "typesOverrides": {
      "date": {
        "return": "string"
      },
      "int8": "BigInt.t"    
    },
    "srcDir": "./src/",
    "dbUrl": "postgres://postgres:password@localhost/postgres"
  }